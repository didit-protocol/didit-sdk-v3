{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:clean": {
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "watch": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "typecheck": {},
    "lint": {},
    "lint:fix": {},
    "dev:example": {
      "cache": false
    }
  }
}
